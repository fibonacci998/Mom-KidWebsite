@model List<Model.EF.Menu>
<nav>
    <ul class="nav" style="margin-bottom: -1px;">
        @foreach (var item in Model)
        {
            <li class="nav-item ">
                <a href="@item.Link" target="item.Target" class="nav-link">
                    @item.Text
                </a>
            </li>
        }
    </ul>
</nav>
